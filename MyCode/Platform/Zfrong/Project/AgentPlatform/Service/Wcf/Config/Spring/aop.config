<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:aop="http://www.springframework.net/aop">
  <object id="serviceOperation" type="Spring.Aop.Support.SdkRegularExpressionMethodPointcut, Spring.Aop">
    <property name="pattern" value=".*BussinessService.*"/>
  </object>
  <object id="perfAdvice" type="Zfrong.Framework.Utils.MyMethodInterceptor,Zfrong.Framework.Utils">
    <property name="Log" ref="logger"/>
  </object>
  <object name="logger" type="Spring.Objects.Factory.Config.LogFactoryObject, Spring.Core">
    <property name="logName" value="mylogger"/>
  </object>
  <aop:config>
    <aop:advisor pointcut-ref="serviceOperation" advice-ref="perfAdvice"/>
  </aop:config>
</objects>