      <clear/>
      <add name="DB.DataDB" type="SubSonic.SqlDataProvider, SubSonic" connectionStringName="New Connection String 1" generatedNamespace="DB.DataDB"/>
      <add name="DB.DataWeb" type="SubSonic.SqlDataProvider, SubSonic" connectionStringName="New Connection String 2" generatedNamespace="DB.DataWeb"/>
